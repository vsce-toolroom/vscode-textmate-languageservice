			"expected": true
		},
		{
			"selector": "foo.twentytwo bar.twentytwo - (yo.twentytwo man.twentytwo)",
			"input": ["foo.twentytwo", "bar.twentytwo", "yo.twentytwo", "man.twentytwo"],
			"expected": false
		},
		{
			"selector": "foo.twentythree bar.twentythree - (yo.twentythree | man.twentythree)",
			"input": ["foo.twentythree", "bar.twentythree", "yo.twentythree", "man.twentythree"],
			"expected": false
		},
		{
			"selector": "foo.twentyfour bar.twentyfour - (yo.twentyfour | man.twentyfour)",
			"input": ["foo.twentyfour", "bar.twentyfour", "yo.twentyfour"],
			"expected": false
		}
	],
	"injections": [
		{
			"selector": "R:text.html - (comment.block, text.html source)",
			"input": ["text.html", "bar", "source.css"],
			"expected": false
		},
		{
			"selector": "text.html.php - (meta.embedded | meta.tag), L:text.html.php meta.tag, L:source.js.embedded.html",
			"input": ["text.html.php", "bar", "source.js"],
			"expected": true
		}
	]
