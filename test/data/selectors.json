{
	"simple": [
		{
			"selector": "foo.one",
			"input": ["foo.one"],
			"expected": true
		},
		{
			"selector": "foo.two",
			"input": ["bar.two"],
			"expected": false
		},
		{
			"selector": "bar.three foo.three",
			"input": ["foo.three"],
			"expected": false
		},
		{
			"selector": "bar.four foo.four",
			"input": ["bar.four"],
			"expected": false
		}
	],
	"negated": [
		{
			"selector": "bar.five - foo.five",
			"input": ["foo.five"],
			"expected": false
		},
		{
			"selector": "bar.six - foo.six",
			"input": ["bar.six"],
			"expected": true
		},
		{
			"selector": "bar.seven foo.seven",
			"input": ["bar.seven", "foo.seven"],
			"expected": true
		},
		{
			"selector": "bar.eight - foo.eight",
			"input": ["bar.eight"],
			"expected": true
		},
		{
			"selector": "bar.nine - foo.nine",
			"input": ["foo.nine", "bar.nine"],
			"expected": false
		},
		{
			"selector": "bar.ten - foo.ten",
			"input": ["foo.ten"],
			"expected": false
		}
	],
	"combined": [
		{
			"selector": "bar.eleven, foo.eleven",
			"input": ["foo.eleven"],
			"expected": true
		},
		{
			"selector": "bar.twelve, foo.twelve",
			"input": ["bar.twelve"],
			"expected": true
		},
		{
			"selector": "bar.thirteen, foo.thirteen",
			"input": ["bar.thirteen", "foo.thirteen"],
			"expected": true
		},
		{
			"selector": "bar.fourteen, * - foo.fourteen",
			"input": ["bar.fourteen", "foo.fourteen"],
			"expected": true
		},
		{
			"selector": "bar.fifteen, * - foo.fifteen",
			"input": ["yo.fifteen"],
			"expected": true
		},
		{
			"selector": "bar.sixteen, -foo.sixteen",
			"input": ["foo.sixteen"],
			"expected": false
		}
	],
	"expressions": [
		{
			"selector": "(foo.seventeen)",
			"input": ["foo.seventeen"],
			"expected": true
		},
		{
			"selector": "(foo.eighteen - bar.eighteen)",
			"input": ["foo.eighteen"],
			"expected": true
		},
		{
			"selector": "(foo.nineteen - bar.nineteen)",
			"input": ["foo.nineteen", "bar.nineteen"],
			"expected": false
		},
		{
			"selector": "foo.twenty bar.twenty - (yo.twenty man.twenty)",
			"input": ["foo.twenty", "bar.twenty"],
			"expected": true
		},
		{
			"selector": "foo.twentyone bar.twentyone - (yo.twentyone man.twentyone)",
