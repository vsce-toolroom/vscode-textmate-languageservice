{
  "compilerOptions": {
    "target": "es2019",
    "lib": ["es2019", "dom"],
    "outDir": "../out",
    "typeRoots": ["../node_modules/@types", "../typings"],
    "types": ["node", "webpack-env", "mocha", "imports", "context"],
    "rootDir": "..",
