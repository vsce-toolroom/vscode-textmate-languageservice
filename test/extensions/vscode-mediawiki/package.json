{
  "name": "mediawiki",
  "displayName": "MediaWiki",
  "description": "MediaWiki token service wiring for Textmate language service.",
  "version": "0.0.1",
  "publisher": "sndst00m",
  "engines": {
    "vscode": "^1.51.1"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:typescript"
  ],
  "main": "./out/src/extension.js",
  "browser": "./out/src/extension.js",
  "contributes": {},
  "textmate-languageservice-contributes": {
    "languages": [
      {
        "id": "mediawiki",
        "aliases": [
          "MediaWiki"
        ],
        "extensions": [
          ".mediawiki",
          ".mw",
          ".wiki",
          ".wikitext",
          ".wt"
        ]
      }
    ],
    "grammars": [
      {
        "language": "mediawiki",
        "scopeName": "text.html.mediawiki",
        "path": "./syntaxes/mediawiki.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/vscode": "^1.55.0",
    "@vscode/test-electron": "^2.2.2",
    "@vscode/test-web": "^0.0.34",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "vscode-textmate-languageservice": "file:../../../vscode-textmate-languageservice-2.0.0-recovery.1.tgz"
  }
}
