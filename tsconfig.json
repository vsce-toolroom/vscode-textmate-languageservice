{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2017",
		"outDir": "out",
		"moduleResolution": "node",
		"lib": ["es2017", "dom"],
		"sourceMap": true,
		"resolveJsonModule": true,
		"rootDir": ".",
		"types": ["node", "node-fetch"]
	},
	"exclude": [
		"node_modules",
		".vscode-test",
		"test/vscode-matlab",
		"out"
	]
}
