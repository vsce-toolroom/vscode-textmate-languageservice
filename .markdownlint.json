{
  "code-block-style": {
    "style": "fenced"
  },
  "code-fence-style": {
    "style": "backtick"
  },
  "emphasis-style": {
    "style": "asterisk"
  },
  "fenced-code-language": false,
  "heading-style": {
    "style": "atx"
  },
  "hr-style": {
    "style": "---"
  },
  "line-length": false,
  "no-blanks-blockquote": false,
  "no-duplicate-heading": {
    "siblings_only": true
  },
  "no-hard-tabs": false,
  "no-inline-html": false,
  "ol-prefix": {
    "style": "ordered"
  },
  "proper-names": {
    "code_blocks": false,
    "names": [
      "CommonMark",
      "Ctrl",
      "JavaScript",
      "Markdown",
      "markdown-it",
      "markdownlint",
      "Node.js",
      "Shift",
      "Visual Studio Code"
    ]
  },
  "strong-style": {
    "style": "asterisk"
  },
  "ul-style": {
    "style": "dash"
  }
}
