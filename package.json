{
  "name": "vscode-textmate-languageservice",
  "description": "Textmate token-based language service for Visual Studio Code.",
  "version": "1.1.0",
  "license": "MIT",
  "author": {
    "name": "SNDST00M",
    "email": "sndst00m@outlook.com",
    "url": "https://gitlab.com/SNDST00M/"
  },
  "engines": {
    "vscode": "^1.51.0"
  },
  "keywords": [
    "vscode",
    "vscode-extension",
    "textmate",
    "grammar",
    "tokenizer"
  ],
  "homepage": "https://gitlab.com/SNDST00M/vscode-textmate-languageservice/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/SNDST00M/vscode-textmate-languageservice.git"
  },
  "main": "./dist/src/main.js",
  "browser": "./dist/src/main.js",
  "types": "./dist/types/src/main.d.ts",
  "bugs": "https://gitlab.com/SNDST00M/vscode-textmate-languageservice/issues",
  "scripts": {
    "prepublishOnly": "npm-run-all test bundle clean-production",
    "test": "npm-run-all reset patch stage harness",
    "posttest": "npm-run-all dump reset",
    "clean": "shx rm -rf dist",
    "clean-production": "shx rm -rf dist/test",
    "pegjs": "node ./scripts/pegjs.js",
    "prebundle": "npm-run-all clean pegjs",
    "bundle": "node ./scripts/bundle.js",
    "reset": "npm-run-all reset-matlab reset-typescript",
    "reset-matlab": "node ./scripts/reset.js ./test/vscode-matlab",
    "reset-typescript": "node ./scripts/reset.js ./test/vscode-typescript",
    "patch": "shx cp -r test/vscode-matlab-patches/* test/vscode-matlab",
    "stage": "npm-run-all stage-matlab stage-typescript",
    "stage-matlab": "node ./scripts/stage.js ./test/vscode-matlab",
    "stage-typescript": "node ./scripts/stage.js ./test/vscode-typescript",
    "harness": "npm-run-all harness-rebuild harness-electron harness-web",
    "harness-rebuild": "npm-run-all harness-rebuild-matlab harness-rebuild-typescript",
    "harness-rebuild-matlab": "npm run --prefix ./test/vscode-matlab vscode:prepublish",
    "harness-rebuild-typescript": "npm run --prefix ./test/vscode-typescript vscode:prepublish",
    "harness-electron": "mocha ./test/vscode-matlab/node_modules/vscode-textmate-languageservice/dist/test/harness-electron.js",
    "harness-web": "npm-run-all harness-web-matlab harness-web-typescript",
    "harness-web-matlab": "node ./test/vscode-matlab/node_modules/vscode-textmate-languageservice/dist/test/harness-web.matlab.js",
    "harness-web-typescript": "node ./test/vscode-typescript/node_modules/vscode-textmate-languageservice/dist/test/harness-web.typescript.js",
    "dump": "npm-run-all dump-matlab dump-typescript",
    "dump-typescript": "shx cp -r ./test/vscode-typescript/data/* ./test/data",
    "dump-matlab": "shx cp -r ./test/vscode-matlab/data/* ./test/data"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "12.19.16",
    "@types/webpack-env": "1.18",
    "@vscode/test-electron": "^2.2.2",
    "@vscode/test-web": "^0.0.34",
    "dequal": "^2.0.3",
    "encoded-uint8array-loader": "^1.0.3",
    "glob": "^9.2.1",
    "line-diff": "^2.1.1",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "peggy": "^2.0.1",
    "prettier": "^2.3.1",
    "process": "^0.11.10",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "ts-loader": "^9.4.2",
    "ts-pegjs": "^3.1.0",
    "typescript": "^4.4.3",
    "vscode-oniguruma": "^1.7.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@types/vscode": "^1.51.0",
    "type-fest": "^0.8.1",
    "vscode-textmate": "^9.0.0"
  }
}
