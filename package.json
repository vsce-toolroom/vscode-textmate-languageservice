{
  "name": "vscode-textmate-languageservice",
  "description": "Textmate token-based language service for Visual Studio Code.",
  "version": "1.1.0-alpha.2",
  "license": "MIT",
  "author": {
    "name": "SNDST00M",
    "email": "sndst00m@outlook.com",
    "url": "https://gitlab.com/SNDST00M/"
  },
  "engines": {
    "vscode": "^1.51.0"
  },
  "homepage": "https://gitlab.com/SNDST00M/vscode-textmate-languageservice/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/SNDST00M/vscode-textmate-languageservice.git"
  },
  "main": "./dist/index.js",
  "types": "./out/src/index",
  "bugs": "https://gitlab.com/SNDST00M/vscode-textmate-languageservice/issues",
  "scripts": {
    "prepack": "npm run webpack",
    "prepublishOnly": "npm-run-all webpack production",
    "test": "npm-run-all reset patch stage jest",
    "posttest": "npm-run-all dump reset",
    "clean": "del-cli out dist",
    "parser": "npm-run-all parser-pegjs parser-prettier parser-tspegjs-issue-90-hack",
    "parser-pegjs": "peggy --plugin ts-pegjs --extra-options-file src/parser/pegconfig.json --dependency matchers:./matchers -o src/parser/parser.ts src/parser/parser.pegjs",
    "parser-prettier": "prettier --config .prettierrc.json --write src/parser/parser.ts",
    "parser-tspegjs-issue-90-hack": "replace-in-files --regex='^let(\\s\\w+\\s=\\srequire)' --replacement='import$1' src/parser/parser.ts",
    "prewebpack": "npm-run-all clean parser",
    "webpack": "webpack-cli",
    "production": "del-cli dist/test dist/*.test.js",
    "reset": "npm-run-all reset-git reset-npm reset-pack",
    "reset-git": "git submodule update -f --init",
    "reset-npm": "npm --prefix ./test/vscode-matlab install",
    "reset-pack": "del-cli *.tgz test/vscode-matlab/*.tgz",
    "lint": "eslint --ext .ts src",
    "patch": "cpy * ../vscode-matlab --cwd=test/vscode-matlab-patches --parents",
    "stage": "npm-run-all stage-pack stage-install stage-vscode-test stage-data stage-samples",
    "stage-pack": "npm-run-all stage-pack-tar stage-pack-copy",
    "stage-pack-tar": "npm pack",
    "stage-pack-copy": "cpy *.tgz test/vscode-matlab",
    "stage-install": "glob-run npm --prefix ./test/vscode-matlab i --omit=dev *.tgz",
    "stage-data": "npm-run-all stage-data-touch stage-data-copy",
    "stage-data-touch": "make-dir test/vscode-matlab/data",
    "stage-data-copy": "cpy data/*/*.json vscode-matlab --cwd=test --parents",
    "stage-samples": "npm-run-all stage-samples-touch stage-samples-copy",
    "stage-samples-touch": "make-dir test/vscode-matlab/samples",
    "stage-samples-copy": "cpy samples/*.m vscode-matlab --cwd=test --parents",
    "stage-vscode-test": "npm i --prefix ./test/vscode-matlab @vscode/test-electron@^2.2.0 @vscode/test-web@^0.0.34",
    "jest": "npm-run-all jest-electron jest-web",
    "jest-electron": "node ./test/vscode-matlab/node_modules/vscode-textmate-languageservice/dist/runner-electron.test.js",
    "jest-web": "node ./test/vscode-matlab/node_modules/vscode-textmate-languageservice/dist/runner-web.test.js",
    "dump": "cpy */*.json ../../data --cwd=test/vscode-matlab/data --parents"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@vscode/test-electron": "^2.2.0",
    "@vscode/test-web": "^0.0.34",
    "copy-webpack-plugin": "^11.0.0",
    "cpy-cli": "^3.1.1",
    "deep-equal": "^1.1.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "glob-run": "^0.1.7",
    "jest": "^29.4.2",
    "make-dir-cli": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "peggy": "^2.0.1",
    "prettier": "^2.3.1",
    "replace-in-files-cli": "^1.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-pegjs": "^3.0.0",
    "typescript": "^4.4.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@types/node": "^16.14.2",
    "@types/vscode": "^1.51.0",
    "type-fest": "^0.6.0",
    "vscode-oniguruma": "^1.7.0",
    "vscode-textmate": "^7.0.0"
  }
}
