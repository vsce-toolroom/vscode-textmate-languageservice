{
  "name": "vscode-textmate-languageservice",
  "description": "Textmate language service for Visual Studio Code.",
  "version": "0.2.3",
  "license": "MIT",
  "author": {
    "name": "SNDST00M",
    "email": "sndst00m@outlook.com",
    "url": "https://gitlab.com/SNDST00M/"
  },
  "engines": {
    "vscode": "^1.51.0"
  },
  "homepage": "https://gitlab.com/SNDST00M/vscode-textmate-languageservice/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/SNDST00M/vscode-textmate-languageservice.git"
  },
  "main": "./out/src/index.js",
  "types": "./out/src/index",
  "bugs": "https://gitlab.com/SNDST00M/vscode-textmate-languageservice/issues",
  "scripts": {
    "test": "npm-run-all compile patch stage mocha",
    "posttest": "npm-run-all dump reset",
    "build": "npm-run-all pegjs compile",
    "precompile": "del-cli out",
    "compile": "npx tsc -p ./",
    "dump": "cpy test/vscode-matlab/node_modules/vscode-textmate-languageservice/out/test/data/** test/data",
    "lint": "eslint --ext .ts src",
    "mocha": "cd test/vscode-matlab/node_modules/vscode-textmate-languageservice && mocha out/test/runner --timeout 0 && cd ../../../..",
    "patch": "cpy test/data/vscode-matlab-patches/** test/vscode-matlab",
    "pegjs": "npx peggy --plugin ts-pegjs --extra-options-file src/scopes/parser.pegconfig.json -o src/scopes/parser.ts src/scopes/parser.pegjs",
    "stage-touchdata": "make-dir test/vscode-matlab/node_modules/vscode-textmate-languageservice/out/test/data",
    "stage-data": "cpy test/data/** test/vscode-matlab/node_modules/vscode-textmate-languageservice/out/test/data",
    "stage-install": "npm pack --pack-destination test/vscode-matlab && cd test/vscode-matlab && npm install --production vscode-textmate-languageservice-0.2.3.tgz && cd ../..",
    "stage-mocha": "cd test/vscode-matlab/node_modules/vscode-textmate-languageservice && npm install mocha && cd ../../../..",
    "stage": "npm-run-all stage-install stage-mocha stage-touchdata stage-data",
    "reset": "cd test/vscode-matlab && npm install vscode-textmate-languageservice@0.2.3 && npm un mocha && cd ../.."
  },
  "devDependencies": {
    "@types/deep-equal": "^1.0.1",
    "@types/first-mate": "^7.0.6",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^14.18.11",
    "@types/vscode": "^1.51.0",
    "@vscode/test-electron": "^2.2.0",
    "cpy-cli": "^4.2.0",
    "deep-equal": "^1.1.0",
    "del-cli": "^5.0.0",
    "load-json-file": "^6.2.0",
    "make-dir-cli": "^3.0.0",
    "mocha": "^9.1.1",
    "node-fetch": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "peggy": "^2.0.1",
    "rimraf": "^3.0.2",
    "ts-pegjs": "^3.0.0",
    "type-fest": "^0.6.0",
    "typescript": "^4.4.3",
    "write-json-file": "^4.3.0"
  },
  "dependencies": {
    "browser-or-node": "^2.1.1",
    "delay": "^5.0.0",
    "git-sha1": "^0.1.2",
    "glob": "^7.2.0",
    "vscode-oniguruma": "^1.5.1",
    "vscode-textmate": "^5.4.1"
  }
}
